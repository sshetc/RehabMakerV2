<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RehabMakerV2.Pages.SplashPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <Image x:Name="splashImage" Source="LogoSvg" VerticalOptions="Center" HorizontalOptions="Center" />
            <Image x:Name="RehabMker"  Source="RehabMaker" VerticalOptions="Center" HorizontalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>